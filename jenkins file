node('free-script-11') {
    
    stage('git clone') {
        git branch: 'main', url: 'https://github.com/virtualmail-rgb/spring-petclinic.git'
    }
    stage('build') {
        sh 'mvn package'
    }
    stage('artifacts') {
        archive 'target/*.jar'
    }
    stage('test') {
        junit '**/surefire-reports/*.xml'
    }
}